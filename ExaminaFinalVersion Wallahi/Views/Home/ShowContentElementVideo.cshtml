@model ExaminaFinalVersion.Models.Video

@{
    ViewData["Title"] = "ShowContentElementVideo";
    Layout = "~/_Layout4.cshtml";
}
<div style="width: 100%; height: 100px; background: none;"></div>
<div class="PersonContent">
    <div class="PersonDetails">
        <p>DataID       :     @(Model.Video_ID)</p>
        <p>Title        :     @(Model.Video_Title)</p>
        <p>Script       :     @(Model.Video_script)</p>
        <p>CourseName   :     @(ViewBag.CourseName)</p>
    </div>
    <div style="width: 100%; height: 100px; background: none;"></div>
    <div class="PersonCourses">
        <div class="CoursesElements" style="height:600px !important">
            <video width="320" height="240" controls>
                <source src="~/CoursesData/@(Model.Video_Distenation) type="video/mp4">
                Your browser does not support the video tag.
            </video>
        </div>
    </div>

</div>