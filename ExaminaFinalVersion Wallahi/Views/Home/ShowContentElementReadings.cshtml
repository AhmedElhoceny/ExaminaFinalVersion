@model ExaminaFinalVersion.Models.Reading

@{
    Layout = "~/_Layout4.cshtml";
}
<div style="width: 100%; height: 100px; background: none;"></div>
<div class="PersonContent">
        <div class="PersonDetails">
            <p>DataID       :     @(Model.Reading_ID)</p>
            <p>Title        :     @(Model.Readings_Reading_Title)</p>
            <p>Script       :     @(Model.Readings_Script)</p>
            <p>CourseName   :     @(ViewBag.CourseName)</p>
        </div>
        <div style="width: 100%; height: 100px; background: none;"></div>
        <div class="PersonCourses">
            <div class="CoursesElements" style="height:600px !important ; text-align:center">
                <p style="font-size:25px ; color:#0ead46 ; align-self:center">@(Model.Readings_Content)</p>
            </div>
        </div>

</div>